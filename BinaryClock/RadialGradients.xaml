<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <RadialGradientBrush x:Key="RadialGradientGreen">
        <GradientStop Offset="0.0" Color="#FF00A20E" />
        <GradientStop Offset="0.95" Color="#FF006407" />
    </RadialGradientBrush>
    <RadialGradientBrush x:Key="RadialGradientBlack">
        <GradientStop Offset="0.0" Color="#FF323232" />
        <GradientStop Offset="0.95" Color="#FF000000" />
    </RadialGradientBrush>
</ResourceDictionary>